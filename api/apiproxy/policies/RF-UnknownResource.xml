<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault continueOnError="false" enabled="true" name="RF-UnknownResource">
  <DisplayName>RF-UnknownResource</DisplayName>
  <Properties/>
  <FaultResponse>
    <Set>
      <Payload contentType="application/json">
        {
          "Error": {
            "status": "{propertyset.error.status1}",
            "code": "{propertyset.error.code1}",
            "title": "{propertyset.error.title1}",
            "message": "{propertyset.error.message1}",
            "dateTime": "{formattedTime}"
          }
        } 
      </Payload>
      <StatusCode>404</StatusCode>
      <ReasonPhrase>NOT FOUND</ReasonPhrase>
    </Set>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
