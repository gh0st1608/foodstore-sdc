<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault continueOnError="false" enabled="true" name="RF-VerifyApiKey">
  <DisplayName>RF-VerifyApiKey</DisplayName>
  <Properties/>
  <FaultResponse>
    <Set>
      <Headers/>
      <Payload contentType="application/json">
        {
          "Error": {
            "status": "{propertyset.error.status2}",
            "code": "{propertyset.error.code2}",
            "title": "{propertyset.error.title2}",
            "message": "{propertyset.error.message2}",
            "dateTime": "{formattedTime}"
          }
        } 
      </Payload>
      <StatusCode>401</StatusCode>
      <ReasonPhrase>Unauthorized</ReasonPhrase>
    </Set>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
