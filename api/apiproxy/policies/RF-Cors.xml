<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault continueOnError="false" enabled="true" name="RF-Cors">
  <DisplayName>RF-Cors</DisplayName>
  <Properties/>
  <FaultResponse>
    <Set>
      <Payload contentType="application/json">
        {
          "Error": {
            "status": "{propertyset.error.status5}",
            "code": "{propertyset.error.code5}",
            "title": "{propertyset.error.title5}",
            "message": "{propertyset.error.message5}",
            "dateTime": "{formattedTime}"
          }
        } 
      </Payload>
      <StatusCode>403</StatusCode>
      <ReasonPhrase>FORBIDDEN</ReasonPhrase>
    </Set>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
