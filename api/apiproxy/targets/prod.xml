<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="prod">
  <Description/>
  <FaultRules/>
  <PreFlow name="PreFlow">
    <Request>
      <Step>
        <Name>AM-TargetCopyPathsuffix</Name>
      </Step>
    </Request>
    <Response/>
  </PreFlow>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <Flows/>
  <HTTPTargetConnection>
    <LoadBalancer>
      <Server name="ts-prod-apiscore"/>
    </LoadBalancer>
    <Path>/{proxy.name}{proxy.pathsuffix}</Path>
  </HTTPTargetConnection>
</TargetEndpoint>
