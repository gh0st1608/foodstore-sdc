    <Policy>SA-RateLimit</Policy>
    <Policy>SetFormattedDateTime</Policy>
    <Policy>VA-VerifyApiKey</Policy>
  </Policies>
  <ProxyEndpoints>
    <ProxyEndpoint>triages</ProxyEndpoint>
  </ProxyEndpoints>
  <Resources>
    <Resource>jsc://JS-HostValidation.js</Resource>
    <Resource>jsc://JS-RegexTP.js</Resource>
    <Resource>oas://Request-Triages-Validation.yaml</Resource>
